{
  "name": "module-lens",
  "version": "0.1.6",
  "description": "Simple module specifier utilities.",
  "repository": {
    "type": "git",
    "url": "https://github.com/makeflow/module-lens.git"
  },
  "license": "MIT",
  "author": "Chengdu Mufan Technology Co., Ltd.",
  "main": "bld/library/index.js",
  "types": "bld/library/index.d.ts",
  "files": [
    "src/library/**/*.ts",
    "bld/library",
    "!*.tsbuildinfo"
  ],
  "scripts": {
    "build": "rimraf {bld,.bld-cache} && tsc --build tsconfig.json",
    "lint": "eslint .",
    "lint-prettier": "prettier --list-different \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "yarn lint-prettier && yarn build && yarn lint && ject --coverage",
    "report-coverage": "cat coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "@mufan/code": "^0.2.5",
    "@mufan/eslint-plugin": "^0.1.32",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "coveralls": "^3.1.0",
    "eslint": "^7.7.0",
    "jest": "^26.4.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  }
}
